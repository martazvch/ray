fn main() {
    for a in [1, 2, 3] {
        print a
    }

    let arr = [5, 6, 7, 8]
    for a in arr {
        print a
    }
    print "----"

    for i in [1, 2, 3] {
        print i
        for j in [4, 5] {
            print j
        }
    }
    print "----"

    for c in "Martin" {
        print c
    }

    let string = "Tom"
    for c in string {
        print c
    }
    print "----"

    for c in "Martin" {
        print c
        for cc in "Tom" {
            print cc
        }
    }
    print "----"

    for c in "Martin" {
        print c
        for j in [4, 5] {
            print j
        }
    }
    print "----"

    for i in 0..5 {
        print i
    }
    print "----"

    for i in 5..0 {
        print i
    }
    print "----"

    let a = 6

    for i in 0..a {
        print i
    }
    print "----"

    for i in a..0 {
        print i
    }
    print "----"

    for i in -1..5 {
        print i
    }
    print "----"

    for i in -8..-5 {
        print i
    }
    print "----"

    // Labelled
    loop: for val in [1, 2, 3] {
        print val
        if val == 2 do break :loop
    }
    loop: for val in [1, 2, 3] {
        print val
        if val == 2 do break
    }
    print "----"

    outer: {
        loop: for val in [1, 2, 3] {
            print val
            if val == 1 do break
        }

        print "outer"
    }

    outer: {
        loop: for val in [1, 2, 3] {
            print val
            if val == 1 do break :outer
        }

        print "outer"
    }
    print "----"
}

