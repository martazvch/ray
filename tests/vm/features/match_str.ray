fn testString(s: str) {
    print match s {
        "To" => "yup"
        "Martin" => "lom"
        "Tom" => "atLeast"
        _ => "z"
    }
}

fn main() {
    testString("Tom"[0..2])
    testString("Martin")
    testString("Tor")
    testString("Tom")

    print match "Tom" {
        "To" => "yup"
        "Martin" => "Tom"
        "Tom" => "atLeast"
        _ => "z"
    }
}


