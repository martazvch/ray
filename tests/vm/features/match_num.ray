fn matchIntValue(value: int) {
    print match value {
        -5 => -20
        -8..-2 => -10
        0 => 10
        2 => 30
        3 => 30
        1..8 => 90
        _ => 100
    }
}

fn matchFloatValue(value: float) {
    print match value {
        -1.2 => -10
        0.1 => 10
        1.3..8.1 => 80
        -10.3..-8.1 => 90
        _ => 100
    }
}

fn main() {
    matchIntValue(-5) // -20
    matchIntValue(-6) // -10
    matchIntValue(2)  // 30
    matchIntValue(7)  // 90
    matchIntValue(25) // 100

    matchFloatValue(-1.2) // -10
    matchFloatValue(0.1) // 10
    matchFloatValue(3.14) // 80
    matchFloatValue(-9.2) // 90
    matchFloatValue(55.1) // 100
}

