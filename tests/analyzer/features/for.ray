fn main() {
    for a in [1, 2, 3] {
        print a
    }

    let arr = [5, 6, 7, 8]
    for a in arr {
        print a
    }

    let string = "Tom"
    for c in string {
        print c
    }

    for c in "Martin" {
        print c
    }

    for i, v in [] {
        print i
    }

    for v in 0..5 {
        print v
    }

    for i in -1..5 {
        print i
    }

    for i in -8..-5 {
        print i
    }

    loop: for c in "Martin" {
        break :loop
    }
}

