fn main() {
    let value = 2

    print match value {
        -1 => -10
        0 => 10
        2 => 30
        3 => 30
        1..8 => 90
        _ => 100
    }

    let fvalue = 2.0

    print match fvalue {
        -1.2 => -10
        0.1 => 10
        1.3..8.1 => 90
        -10.3..-8.1 => 90
        _ => 100
    }
}

